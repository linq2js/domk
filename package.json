    "cross-env": "^7.0.2",
    "jest": "^26.2.2",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
